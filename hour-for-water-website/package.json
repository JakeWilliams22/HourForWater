{
  "name": "hour-for-water-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-loader": "*",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "semantic-ui-react": "^0.84.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
